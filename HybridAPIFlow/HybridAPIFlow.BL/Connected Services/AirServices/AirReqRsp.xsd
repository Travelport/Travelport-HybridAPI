    <xs:complexContent mixed="false">
      <xs:extension base="common:BaseRsp">
        <xs:sequence>
          <xs:element ref="AirItinerary">
            <xs:annotation>
              <xs:documentation>Provider: 1G,1V,1P,1J,ACH.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="16" ref="AirPriceResult">
            <xs:annotation>
              <xs:documentation>Provider: 1G,1V,1P,1J,ACH.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="AirPriceRsp">
    <xs:complexType>
      <xs:complexContent mixed="false">
        <xs:extension base="BaseAirPriceRsp" />
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
  <xs:element name="AirRepriceReq">
    <xs:annotation>
      <xs:documentation>Request to reprice a solution.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:complexContent mixed="false">
        <xs:extension base="AirBaseReq">
          <xs:sequence>
            <xs:element minOccurs="0" ref="AirReservationLocatorCode" />
            <xs:element ref="AirPricingSolution" />
          </xs:sequence>
          <xs:attribute default="none" name="FareRuleType" type="typeFareRuleType" use="optional" />
          <xs:attribute default="false" name="IgnoreAvailability" type="xs:boolean" use="optional" />
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
  <xs:element name="AirRepriceRsp">
    <xs:complexType>
      <xs:complexContent mixed="false">
        <xs:extension base="common:BaseRsp">
          <xs:sequence>
            <xs:element ref="AirPricingSolution" />
            <xs:element minOccurs="0" maxOccurs="999" ref="FareRule" />
          </xs:sequence>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
  <xs:element name="AirTicketingReq">
    <xs:annotation>
      <xs:documentation>Request to ticket a previously stored reservation.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:complexContent mixed="false">
        <xs:extension base="AirBaseReq">
          <xs:sequence>
            <xs:element ref="AirReservationLocatorCode">
              <xs:annotation>
                <xs:documentation>Provider: 1G,1V,1P,1J.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" maxOccurs="999" name="AirPricingInfoRef">
              <xs:annotation>
                <xs:documentation>Provider: 1G,1V,1P,1J-Indicates air pricing infos to be ticketed.</xs:documentation>
              </xs:annotation>
              <xs:complexType>
                <xs:sequence>
                  <xs:element minOccurs="0" maxOccurs="9" ref="common:BookingTravelerRef" />
                </xs:sequence>
                <xs:attribute name="Key" type="common:typeRef" use="required" />
              </xs:complexType>
            </xs:element>
            <xs:element minOccurs="0" maxOccurs="999" name="TicketingModifiersRef" type="typeTicketingModifiersRef">
              <xs:annotation>
                <xs:documentation>Provider: 1P,1J-Reference to a shared list of Ticketing Modifiers. This is supported for Worldspan and JAL providers only. When AirPricingInfoRef is used along with TicketingModifiersRef means that particular TicketingModifiers will to be applied while ticketing the Stored fare corresponding to the AirPricingInfo. Absence of AirPricingInfoRef means that particular TicketingModifiers will be applied to all Stored fares which are requested to be ticketed.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" ref="WaiverCode" />
            <xs:element minOccurs="0" maxOccurs="18" ref="common:Commission" />
            <xs:element minOccurs="0" maxOccurs="999" ref="DetailedBillingInformation">
              <xs:annotation>
                <xs:documentation>Provider: 1G,1V.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" ref="FaxDetailsInformation">
              <xs:annotation>
                <xs:documentation>Provider: 1V.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" maxOccurs="999" ref="AirTicketingModifiers">
              <xs:annotation>
                <xs:documentation>Provider: 1G,1V,1P,1J.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" maxOccurs="999" ref="AirSegmentTicketingModifiers">
              <xs:annotation>
                <xs:documentation>Provider: 1P,1J.</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
          <xs:attribute default="true" name="ReturnInfoOnFail" type="xs:boolean" use="optional" />
          <xs:attribute default="false" name="BulkTicket" type="xs:boolean" use="optional">
            <xs:annotation>
              <xs:documentation>Provider: 1G,1V,1P,1J.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute default="false" name="ValidateSpanishResidency" type="xs:boolean" use="optional">
            <xs:annotation>
              <xs:documentation>
									Provider: 1G - If set as true, Spanish Residency will be validated for
									Provisioned Customers.
							</xs:documentation>
            </xs:annotation>
          </xs:attribute>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
  <xs:element name="AirTicketingRsp">
    <xs:annotation>
      <xs:documentation>Response to ticket a previously stored reservation.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:complexContent mixed="false">
        <xs:extension base="common:BaseRsp">
          <xs:choice>
            <xs:element minOccurs="0" ref="AirSolutionChangedInfo" />
            <xs:sequence>
              <xs:element minOccurs="0" maxOccurs="999" ref="ETR">
                <xs:annotation>
                  <xs:documentation>Provider: 1G,1V,1P,1J.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element minOccurs="0" maxOccurs="999" ref="TicketFailureInfo">
                <xs:annotation>
                  <xs:documentation>Provider: 1G,1V,1P,1J.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element minOccurs="0" maxOccurs="999" ref="DetailedBillingInformation">
                <xs:annotation>
                  <xs:documentation>Provider: 1G,1V,1P,1J.</xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:sequence>
          </xs:choice>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
  <xs:complexType name="BaseAirExchangeQuoteReq">
    <xs:complexContent mixed="false">
      <xs:extension base="common:BaseCoreReq">
        <xs:sequence>
          <xs:element minOccurs="0" maxOccurs="999" ref="common:TicketNumber" />
          <xs:element minOccurs="0" name="ProviderReservationInfo">
            <xs:annotation>
              <xs:documentation>Provider: 1G/1V/1P/ACH - Represents a valid Provider Reservation/PNR whose itinerary is to be exchanged</xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:attributeGroup ref="common:ProviderReservation" />
            </xs:complexType>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="2" ref="AirPricingSolution" />
          <xs:element minOccurs="0" ref="AirExchangeModifiers">
            <xs:annotation>
              <xs:documentation>Provider: ACH.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="999" ref="common:HostToken">
            <xs:annotation>
              <xs:documentation>Provider: ACH.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" ref="OptionalServices">
            <xs:annotation>
              <xs:documentation>Provider: ACH.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="999" ref="common:FormOfPayment">
            <xs:annotation>
              <xs:documentation>Provider: ACH-This would allow a user to see the fees if they are changing from one Form Of Payment to other .</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="1" ref="RepricingModifiers" />
          <xs:element minOccurs="0" maxOccurs="1" ref="OriginalItineraryDetails" />
          <xs:element minOccurs="0" maxOccurs="1" ref="PCC" />
        </xs:sequence>
        <xs:attribute default="none" name="FareRuleType" type="typeFareRuleType" use="optional">
          <xs:annotation>
            <xs:documentation>Provider: ACH.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="AirExchangeQuoteReq">
    <xs:annotation>
      <xs:documentation>Request to quote the exchange of an itinerary</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:complexContent mixed="false">
        <xs:extension base="BaseAirExchangeQuoteReq" />
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
  <xs:element name="AirExchangeQuoteRsp">
    <xs:complexType>
      <xs:complexContent mixed="false">
        <xs:extension base="common:BaseRsp">
          <xs:sequence>
            <xs:element minOccurs="0" maxOccurs="999" ref="common:TicketNumber" />
            <xs:element maxOccurs="999" ref="AirPricingSolution">
              <xs:annotation>
                <xs:documentation>Provider: 1G/1V/1P/1S/1A.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" maxOccurs="1" ref="AirExchangeBundleTotal" />
            <xs:element minOccurs="0" maxOccurs="999" ref="AirExchangeBundle">
              <xs:annotation>
                <xs:documentation>Bundle exchange, pricing, and penalty information. Providers ACH/1G/1V/1P</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" maxOccurs="999" ref="common:HostToken">
              <xs:annotation>
                <xs:documentation>Encrypted data from the host. Required to send the HostToken from the AirExchangeQuoteRsp in the AirExchangeReq. Providers ACH/1G/1V/1P.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" ref="OptionalServices">
              <xs:annotation>
                <xs:documentation>Provider: ACH.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" maxOccurs="999" ref="FareRule">
              <xs:annotation>
                <xs:documentation>Provider: ACH.</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
  <xs:element name="AirExchangeReq">
    <xs:annotation>
      <xs:documentation>Request to exchange an itinerary</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:complexContent mixed="false">
        <xs:extension base="common:BaseReq">
          <xs:sequence>
            <xs:element ref="AirReservationLocatorCode">
              <xs:annotation>
                <xs:documentation>Identifies the PNR locator code. Providers ACH/1G/1V/1P</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" maxOccurs="999" ref="common:TicketNumber" />
            <xs:element minOccurs="0" maxOccurs="999" ref="SpecificSeatAssignment">
              <xs:annotation>
                <xs:documentation>Identifies the standard seat. Providers ACH/1G/1V/1P</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="999" ref="AirPricingSolution">
              <xs:annotation>
                <xs:documentation>Providers ACH/1G/1V/1P.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" ref="AirExchangeModifiers">
              <xs:annotation>
                <xs:documentation>Provider: ACH.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" maxOccurs="1" ref="AirExchangeBundleTotal">
              <xs:annotation>
                <xs:documentation>Provider: 1G/1V/1P/1S/1A.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" maxOccurs="999" ref="AirExchangeBundle">
              <xs:annotation>
                <xs:documentation>Bundle exchange, pricing, and penalty information. Providers ACH/1G/1V/1P.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" maxOccurs="999" ref="common:HostToken">
              <xs:annotation>
                <xs:documentation>Encrypted data from the host. Required to send the HostToken from the AirExchangeQuoteRsp in the AirExchangeReq. Providers ACH/1G/1V/1P</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" ref="OptionalServices">
              <xs:annotation>
                <xs:documentation>Provider: ACH.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" ref="common:FormOfPayment">
              <xs:annotation>
                <xs:documentation>Form of Payment for any additional collection charges for the Exchange. For ACH, most carriers will only allow refund amounts to the original form of payment. Providers ACH/1G/1V/1P</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" ref="common:FormOfPaymentRef">
              <xs:annotation>
                <xs:documentation>Provider: ACH-Universal Record reference to Form of Payment for any Additional Collection charges or Refund due for the itinerary exchange</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" maxOccurs="999" ref="common:SSRInfo">
              <xs:annotation>
                <xs:documentation>Providers ACH, 1G, 1V, 1P.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" maxOccurs="1" ref="common:AddSvc">
              <xs:annotation>
                <xs:documentation>1P - Add SVC segments to collect additional fee</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
          <xs:attribute default="false" name="ReturnReservation" type="xs:boolean" use="optional">
            <xs:annotation>
              <xs:documentation>Provider: ACH.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
  <xs:element name="AirExchangeRsp">
    <xs:complexType>
      <xs:complexContent mixed="false">
        <xs:extension base="common:BaseRsp">
          <xs:sequence>
            <xs:element minOccurs="0" maxOccurs="999" ref="common:TicketNumber" />
            <xs:element minOccurs="0" maxOccurs="999" ref="common:BookingTraveler">
              <xs:annotation>
                <xs:documentation>Provider: ACH.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="AirReservation" type="typeAirReservationWithFOP">
              <xs:annotation>
                <xs:documentation>Provider: ACH.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" maxOccurs="999" name="ExchangeFailureInfo" type="typeTicketFailureInfo">
              <xs:annotation>
                <xs:documentation>Provider: ACH.</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
  <xs:element name="AirExchangeTicketingReq">
    <xs:annotation>
      <xs:documentation>Request to ticket an exchanged itinerary. Providers 1G, 1V, 1P.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:complexContent mixed="false">
        <xs:extension base="common:BaseReq">
          <xs:sequence>
            <xs:element ref="AirReservationLocatorCode">
              <xs:annotation>
                <xs:documentation>Identifies the PNR to ticket. Providers 1G, 1V, 1P.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element ref="common:TicketNumber">
              <xs:annotation>
                <xs:documentation>Ticket number to reissue. Providers 1G, 1V, 1P.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" maxOccurs="999" name="TicketingModifiersRef" type="typeTicketingModifiersRef">
              <xs:annotation>
                <xs:documentation>Provider: 1P-Reference to a shared list of Ticketing Modifiers. This is supported for Worldspan provider only. When AirPricingInfoRef is used along with TicketingModifiersRef means that particular TicketingModifiers will to be applied while ticketing the Stored fare corresponding to the AirPricingInfo. Absence of AirPricingInfoRef means that particular TicketingModifiers will be applied to all Stored fares which are requested to be ticketed.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" ref="WaiverCode" />
            <xs:element minOccurs="0" maxOccurs="999" ref="DetailedBillingInformation">
              <xs:annotation>
                <xs:documentation>Providers 1G, 1V, 1P.</xs:documentation>
              </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" maxOccurs="999" ref="AirTicketingModifiers">
              <xs:annotation>
                <xs:documentation>Provider: 1G,1V,1P.</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
          <xs:attribute default="false" name="BulkTicket" type="xs:boolean" use="optional">
            <xs:annotation>
              <xs:documentation>Providers 1G, 1V, 1P.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute default="true" name="ChangeFeeOnTicket" type="xs:boolean" use="optional">
            <xs:annotation>
              <xs:documentation>Applies the change fee/penalty to the original form of payment. Providers: 1V</xs:documentation>
            </xs:annotation>
          </xs:attribute>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
  <xs:element name="AirExchangeTicketingRsp">
    <xs:annotation>
