            </xs:restriction>
          </xs:simpleType>
        </xs:attribute>
        <xs:attribute default="false" name="FareInfoRules" type="xs:boolean" use="optional">
          <xs:annotation>
            <xs:documentation>Returns ChangePenalty and CancelPenalty values at the FareInfo level. If FareRulesFilterCategory is sent FareRulesFilter will be returned at FareInfo level.  Provider: 1G/1V.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute default="false" name="MostRestrictivePenalties" type="xs:boolean" use="optional">
          <xs:annotation>
            <xs:documentation>Boolean flag used to request the MostRestrictivePenalties in the response</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
